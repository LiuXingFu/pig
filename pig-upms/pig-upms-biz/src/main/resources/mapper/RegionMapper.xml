<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pig4cloud.pig.admin.mapper.RegionMapper">

  <resultMap id="regionMap" type="com.pig4cloud.pig.admin.api.entity.Region">
    <id property="id" column="id"/>
    <result property="code" column="code"/>
    <result property="name" column="name"/>
    <result property="shortName" column="short_name"/>
    <result property="mergeName" column="merge_name"/>
    <result property="mergeShortName" column="merge_short_name"/>
    <result property="parentId" column="parent_id"/>
    <result property="pinyin" column="pinyin"/>
    <result property="fullpinyin" column="fullpinyin"/>
    <result property="cityCode" column="city_code"/>
    <result property="zipCode" column="zip_code"/>
    <result property="longitude" column="longitude"/>
    <result property="latitude" column="latitude"/>
    <result property="firstLetter" column="first_letter"/>
    <result property="regionLevel" column="region_level"/>
    <result property="countryCode" column="country_code"/>
    <result property="isMunicipality" column="is_municipality"/>
    <result property="status" column="status"/>
    <result property="delFlag" column="del_flag"/>
    <result property="modifyBy" column="modify_by"/>
    <result property="modifyTime" column="modify_time"/>
    <result property="tenantId" column="tenant_id"/>
  </resultMap>

</mapper>
